<main>
    {{#each blogPosts as |post|}}
        <div id="blog-post">
            <div id="post-header">
                <h2>{{post.post_name}}</h2>
                <p>{{post.user.name}}</p>
            </div>
            <div id="post-preview">
                <p>{{post.post_content}}</p>
            </div>
        </div>
    {{/each}}
</main>